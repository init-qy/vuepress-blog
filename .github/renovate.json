{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base", ":dependencyDashboard"],
  "labels": ["dependencies"],
  "packageRules": [
    {
      "packageNames": ["vuepress", "@vuepress/client"],
      "allowedVersions": "/^((?!pre).)*$/"
    },
    {
      "matchPackagePatterns": "^@?vue",
      "groupName": ["vue packages"]
    }
  ]
}
