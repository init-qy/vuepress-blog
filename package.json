{
  "name": "vuepress-blog",
  "version": "0.0.1",
  "description": "a blog made of vuepress",
  "keywords": [
    "vuepress",
    "blog"
  ],
  "homepage": "https://init-qy.github.io/vuepress-blog",
  "license": "MIT",
  "author": "init-qy",
  "scripts": {
    "docs:build": "vuepress build docs --clean-temp --clean-cache",
    "docs:dev": "vuepress dev docs",
    "docs:dev-nocache": "vuepress dev docs --clean-temp --clean-cache",
    "docs:preview": "vite preview docs/.vuepress",
    "lint": "pnpm lint:eslint && pnpm lint:md && pnpm lint:prettier",
    "lint:eslint": "eslint --ext .js,.vue,.ts --fix .",
    "lint:md": "markdownlint docs/ --fix",
    "lint:prettier": "prettier --check --write .",
    "prepare": "husky install",
    "sync:en-docs": "cpy 'docs/posts/*' docs/en/posts",
    "up": "taze major -I"
  },
  "lint-staged": {
    "*.md": [
      "markdownlint --fix",
      "prettier --write"
    ],
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{json,yml}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "@vueuse/core": "10.2.1",
    "color": "4.2.3",
    "echarts": "5.4.2",
    "lodash-es": "4.17.21",
    "naive-ui": "^2.34.4",
    "rgbaster": "2.1.1",
    "typeit": "8.7.1",
    "vue-skeletor": "1.0.6",
    "wasm-feature-detect": "^1.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.39.7",
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@types/color": "3.0.3",
    "@vuepress/client": "2.0.0-beta.64",
    "cpy-cli": "^5.0.0",
    "eslint": "8.44.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "markdownlint-cli": "0.35.0",
    "prettier": "3.0.0",
    "sass-loader": "13.3.2",
    "sort-package-json": "2.5.0",
    "taze": "^0.11.2",
    "typescript": "5.1.6",
    "unocss": "^0.53.5",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vue": "3.3.4",
    "vue-router": "4.2.4",
    "vuepress": "2.0.0-beta.64",
    "vuepress-plugin-search-pro": "2.0.0-beta.230",
    "vuepress-theme-hope": "2.0.0-beta.230"
  },
  "packageManager": "pnpm@8.6.6",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom",
        "webpack"
      ]
    }
  }
}
